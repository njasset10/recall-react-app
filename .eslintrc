{
 "extends": "airbnb",
 "plugins": ["babel"],
 "parser": "babel-eslint",
 "rules": {
   "babel/semi": 1,
   "react/prop-types": 0,
   "arrow-parens": 0,
   "no-mixed-operators": 0,
   "function-paren-newline": 0,
   "object-curly-newline": 0,
   "no-unused-vars": "warn",
   "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
 }
}
